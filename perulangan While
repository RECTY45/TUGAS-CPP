#include <iostream>
using namespace std;

int main()
{
    string nama, nim, kelas;
    nama = "Fery Fadul Rahman";
    nim = "20232125013";
    kelas = "S.102";

    cout << "===========================\n";
    cout << "Nama  : " << nama << endl;
    cout << "NIM   : " << nim << endl;
    cout << "Kelas : " << kelas << endl;
    cout << "===========================\n";
    
    int varUlang = 2;
   while(varUlang < 5 ){
       cout << varUlang << " Hellow World" << endl;
       varUlang++;
   }
    
    return 0;
}
