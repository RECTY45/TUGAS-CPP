int main()
{
    string nama, nim, kelas;
    nama = "Fery Fadul Rahman";
    nim = "20232125013";
    kelas = "S.102";

    cout << "===========================\n";
    cout << "Nama  : " << nama << endl;
    cout << "NIM   : " << nim << endl;
    cout << "Kelas : " << kelas << endl;
    cout << "===========================\n";
    
    for(int VarUlang = 1; VarUlang < 8;VarUlang++){
        if(VarUlang == 5 ){
            break;
        }
        cout << VarUlang << " Selamat Belajar Bahasa C++" << endl;
    }
    
    kamu1:
    cout << "Selamat Belajar Bahasa1";
    return 0;
    kamu2 :
    cout << "Selamat Belajar Bahasa2";
    return 0;
}
